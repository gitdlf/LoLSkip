<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">


	<!-- 过滤器配置 -->
	<filter>
		<filter-name>Encoding_Filter</filter-name>
		<filter-class>cn.lfsenior.filter.Encoding_Filter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Encoding_Filter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>SkipList_Servlet</servlet-name>
		<servlet-class>cn.lfsenior.servlet.SkipList_Servlet</servlet-class>
	</servlet>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>PackList_Servlet</servlet-name>
		<servlet-class>cn.lfsenior.servlet.PackList_Servlet</servlet-class>
	</servlet>
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>SkipLoad_Servlet</servlet-name>
		<servlet-class>cn.lfsenior.servlet.SkipLoad_Servlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>PackList_Servlet</servlet-name>
		<url-pattern>/PackList_Servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SkipList_Servlet</servlet-name>
		<url-pattern>/SkipList_Servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SkipLoad_Servlet</servlet-name>
		<url-pattern>/SkipLoad_Servlet</url-pattern>
	</servlet-mapping>



</web-app>